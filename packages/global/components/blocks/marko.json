{
  "taglib-imports": [
    "./promos/marko.json"
  ],
  "<global-content-body-with-tracking>": {
    "template": "./content-body-with-tracking.marko",
    "@content": {
      "type": "object",
      "required": true
    },
    "@block-name": {
      "type": "string",
      "required": true
    }
  },
  "<global-pmg-events-block>": {
    "template": "./pmg-events.marko"
  },
  "<global-native-x-announcement-block>": {
    "template": "./native-x-announcement.marko"
  },
  "<global-native-x-list-block>": {
    "template": "./native-x-list.marko"
  },
  "<global-promo-card-rotation-block>": {
    "template": "./promo-card-rotation.marko"
  },
  "<global-supplier-submitted-block>": {
    "template": "./supplier-submitted.marko",
    "@labels": "array"
  },
  "<global-hero-card-block>": {
    "template": "./hero-card-deck.marko",
    "@alias": "string",
    "@description": "string",
    "@name": "string",
    "@query-params": "object",
    "@section": "object"
  },
  "<global-published-content-list-block>": {
    "template": "./published-content-list.marko",
    "@section-id": "number",
    "@exclude-content-ids": "array"
  },
  "<global-published-content-list-deck-block>": {
    "template": "./published-content-list-deck.marko",
    "@section-ids": "array",
    "@exclude-top-stories-count": "number",
    "@exclude-content-ids": "array"
  },
  "<global-client-side-most-popular-block>": {
    "template": "./client-side-most-popular.marko"
  },
  "<global-company-booth-block>": {
    "template": "./company-booth.marko"
  }
}
