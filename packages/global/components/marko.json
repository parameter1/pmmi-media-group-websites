{
  "taglib-imports": [
    "./blocks/marko.json",
    "./flows/marko.json",
    "./layouts/marko.json",
    "./nodes/marko.json",
    "./leaders/marko.json"
  ],
  "<global-image-slider>": {
    "template": "./image-slider.marko"
  },
  "<theme-content-card-list-block>": {
    "template": "./content-card-list.marko"
  },
  "<global-gam-content-targeting>": {
    "template": "./gam-content-targeting.marko"
  },
  "<global-sponsored-label-logo>": {
    "template": "./sponsored-label-logo.marko"
  },
  "<global-sponsored-section-logo>": {
    "template": "./sponsored-section-logo.marko"
  },
  "<global-sponsored-section-name-logo>": {
    "template": "./sponsored-section-name-logo.marko"
  },

  "<global-site-menu>": {
    "template": "./site-menu/index.marko"
  },
  "<global-document>": {
    "template": "./document.marko",
    "<head>": {},
    "<above-container>": {},
    "<container>": {
      "<above-page>": {},
      "<page>": {},
      "<below-page>": {}
    },
    "<below-container>": {},
    "<foot>": {}
  },
  "<global-site-header>": {
    "template": "./site-header.marko"
  },
  "<marko-web-leaders-featured-companies>": {
    "template": "./featured-companies.marko",
    "@content": {
      "type": "object",
      "required": true
    }
  },
  "<marko-web-leaders-company-videos>": {
    "template": "./company-videos.marko",
    "@content-id": {
      "type": "number",
      "required": true
    }
  }
}
