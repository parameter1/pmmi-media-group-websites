{
  "taglib-imports": [
    "./contact-details/marko.json",
    "./company-details/marko.json"
  ],
  "<global-content-default-layout>": {
    "template": "./default.marko",
    "<after-body>": {},
    "@sections <section>[]": {},
    "@id": "number",
    "@type": "string",
    "@page-node": "object",
    "@show-read-next-block": {
      "type": "boolean",
      "default-value": false
    },
    "@show-bottom-ad-block": {
      "type": "boolean",
      "default-value": true
    },
    "@show-top-story-block": {
      "type": "boolean",
      "default-value": true
    }
  },
  "<global-content-company-layout>": {
    "template": "./company.marko",
    "<after-body>": {},
    "<right-rail>": {},
    "@sections <section>[]": {},
    "@id": "number",
    "@type": "string",
    "@page-node": "object"
  },
  "<global-content-media-gallery-layout>": {
    "template": "./media-gallery.marko",
    "<after-body>": {},
    "@sections <section>[]": {},
    "@id": "number",
    "@type": "string",
    "@page-node": "object"
  },
  "<global-content-whitepaper-layout>": {
    "template": "./whitepaper.marko",
    "@id": "number",
    "@type": "string",
    "@page-node": "object"
  },
  "<global-content-wrapper-layout>": {
    "template": "./wrapper.marko",
    "@sections <section>[]": {},
    "@id": "number",
    "@type": "string",
    "@page-node": "object"
  },
  "<global-content-webinar-layout>": {
    "template": "./webinar.marko",
    "<after-body>": {},
    "@sections <section>[]": {},
    "@id": "number",
    "@type": "string",
    "@page-node": "object",
    "@show-read-next-block": {
      "type": "boolean",
      "default-value": false
    },
    "@show-bottom-ad-block": {
      "type": "boolean",
      "default-value": true
    },
    "@show-top-story-block": {
      "type": "boolean",
      "default-value": true
    }
  }
}
