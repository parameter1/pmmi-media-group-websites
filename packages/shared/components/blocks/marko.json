{
  "<shared-content-body-with-tracking>": {
    "template": "./content-body-with-tracking.marko",
    "@content": {
      "type": "object",
      "required": true
    },
    "@block-name": {
      "type": "string",
      "required": true
    }
  },
  "<shared-content-load-more-block>": {
    "template": "./content-load-more.marko",
    "<native-x>": {
      "@index": "number",
      "@name": "string",
      "@aliases": "array"
    },
    "@id": "number",
    "@type": "string",
    "@name": "string",
    "@aliases": "array",
    "@section-id": "number",
    "@section-name": "string",
    "@section-skip": "number"
  },
  "<shared-primary-image-block>": {
    "template": "./primary-image.marko"
  },
  "<shared-content-media-block>": {
    "template": "./content-media.marko"
  },
  "<shared-inquiry-block>": {
    "template": "./inquiry.marko",
    "@content": "object",
    "@modifiers": "array",
    "@with-header": {
      "type": "boolean",
      "default-value": true
    },
    "@lang": "string"
  },
  "<shared-magazine-publications-block>": {
    "template": "./magazine-publications.marko"
  }
}
